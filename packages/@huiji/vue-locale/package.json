{
  "name": "@huiji/vue-locale",
  "version": "1.2.0",
  "publishConfig": {
    "access": "public"
  },
  "description": "plugin localization for vue.js 2+",
  "main": "dist/common/index.js",
  "module": "dist/esm/index.js",
  "typings": "dist/esm/index.d.ts",
  "sideEffects": false,
  "repository": "https://github.com/HuijiFE/huiji-utils",
  "keywords": [
    "vue",
    "i18n",
    "internationalization",
    "localization"
  ],
  "author": "duduluu <duduluu@outlook.com>",
  "license": "MIT",
  "scripts": {
    "clean:dist": "rimraf ./dist",
    "build": "yarn clean:dist && tsc -p ./tsconfig.esm.json && tsc -p ./tsconfig.cjs.json",
    "lint": "tslint -c ./tslint.json -p ./tsconfig.esm.json -t verbose",
    "test": "jest --maxWorkers=4"
  },
  "dependencies": {
    "tslib": "^1.9.3"
  },
  "peerDependencies": {
    "vue": "^2.5.0"
  }
}
