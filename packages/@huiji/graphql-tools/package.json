{
  "name": "@huiji/graphql-loader",
  "version": "0.0.1",
  "description": "A webpack loader for loading grapqhql query file as plain text",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "sideEffects": false,
  "repository": "https://github.com/HuijiFE/huiji-utils",
  "keywords": [
    "vue",
    "webpack",
    "vebpack-loader",
    "markdown-it",
    "markdown-it-plugin"
  ],
  "author": "duduluu <duduluu@outlook.com>",
  "license": "MIT",
  "scripts": {
    "clean:dist": "rimraf ./dist",
    "build": "yarn clean:dist && tsc -p ./tsconfig.esm.json && tsc -p ./tsconfig.cjs.json",
    "lint": "tslint -c ./tslint.json -p ./tsconfig.esm.json -t verbose",
    "test": "jest --maxWorkers=4"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "loader-utils": "^1.1.0",
    "tslib": "^1.9.3"
  },
  "devDependencies": {
    "@types/loader-utils": "^1.1.3",
    "@types/webpack": "^4.4.11"
  }
