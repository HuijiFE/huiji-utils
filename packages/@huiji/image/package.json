{
  "name": "@huiji/image",
  "version": "1.0.1",
  "description": "a image processing library",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "sideEffects": false,
  "repository": "https://github.com/HuijiFE/huiji-utils",
  "keywords": [
    "image",
    "png",
    "sprites"
  ],
  "author": "duduluu <duduluu@outlook.com>",
  "license": "MIT",
  "scripts": {
    "clean:dist": "rimraf ./dist",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build": "yarn build:cjs && yarn build:esm",
    "lint": "tslint -p tsconfig.json",
    "_test": "jest --maxWorkers=4",
    "ts": "ts-node -r tsconfig-paths/register -P scripts/tsconfig.json"
  },
  "dependencies": {
    "pngjs": "^3.3.3"
  },
  "devDependencies": {
    "@types/pngjs": "^3.3.2"
  }
}
