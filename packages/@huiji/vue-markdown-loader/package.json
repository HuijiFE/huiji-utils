{
  "name": "@huiji/vue-markdown-loader",
  "version": "1.0.1",
  "description": "A webpack loader for loading markdown files as Vue Single-File Components",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "repository": "https://github.com/HuijiFE/huiji-utils",
  "keywords": [
    "vue",
    "webpack",
    "vebpack-loader",
    "markdown-it",
    "markdown-it-plugin"
  ],
  "author": "duduluu <duduluu@outlook.com>",
  "license": "MIT",
  "scripts": {
    "clean:dist": "rimraf ./dist",
    "build": "yarn clean:dist && tsc -p ./tsconfig.project.json",
    "lint": "tslint -c ./tslint.project.json -p ./tsconfig.project.json",
    "test:unit": "jest --maxWorkers=4"
  },
  "dependencies": {
    "loader-utils": "^1.1.0",
    "markdown-it": "^8.4.2",
    "markdown-it-anchor": "^5.0.2",
    "markdown-it-container": "^2.0.0"
  },
  "devDependencies": {
    "@types/loader-utils": "^1.1.3",
    "@types/markdown-it": "^0.0.5",
    "@types/markdown-it-anchor": "^4.0.1",
    "@types/markdown-it-container": "^2.0.0",
    "@types/webpack": "^4.4.11"
  }
}
