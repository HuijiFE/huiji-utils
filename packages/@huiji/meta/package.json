{
  "name": "@huiji/meta",
  "version": "0.1.0",
  "publishConfig": {
    "access": "public"
  },
  "description": "a tool for generating html metadata",
  "main": "dist/common/index.js",
  "module": "dist/esm/index.js",
  "typings": "dist/esm/index.d.ts",
  "sideEffects": false,
  "repository": "https://github.com/HuijiFE/huiji-utils",
  "keywords": [
    "meta",
    "metadata",
    "seo"
  ],
  "author": "duduluu <duduluu@outlook.com>",
  "license": "MIT",
  "scripts": {
    "clean:dist": "rimraf ./dist",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build": "yarn build:cjs && yarn build:esm",
    "lint": "tslint -p tsconfig.json",
    "_test": "jest --maxWorkers=4",
    "ts": "ts-node -r tsconfig-paths/register -P scripts/tsconfig.json"
  },
  "dependencies": {
    "tslib": "^1.9.3"
  }
}
